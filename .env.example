# 藏鋒系統環境變數範例
# 複製此檔案為 .env 並填入實際值

# FinMind API Token (https://finmindtrade.com/ 註冊取得)
FINMIND_TOKEN=your_finmind_token_here

# 資料庫連線
DATABASE_URL=postgresql://zf_user:zf_password@localhost:5432/zf_trend

# Google Sheet 設定
GOOGLE_CREDENTIALS_PATH=credentials.json
SHEET_ID_COMPANY_MASTER=your_sheet_id_here
SHEET_ID_TW_VCP=your_sheet_id_here
SHEET_ID_TW_SANXIAN=your_sheet_id_here

# 日誌級別 (DEBUG/INFO/WARNING/ERROR)
LOG_LEVEL=INFO
